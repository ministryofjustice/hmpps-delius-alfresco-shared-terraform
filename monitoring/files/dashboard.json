                    [
                        "AWS/S3",
                        "BucketSizeBytes",
                        "StorageType",
                        "StandardStorage",
                        "BucketName",
                        "${common_prefix}-alf-backups",
                        {
                            "period": 86400
                        }
                    ]
                ],
                "region": "${region}",
                "period": 300
            }
        },
        {
            "type": "metric",
            "x": 6,
            "y": 12,
            "width": 6,
            "height": 6,
            "properties": {
                "view": "timeSeries",
                "stacked": false,
                "metrics": [
                    [
                        "AWS/RDS",
                        "FreeStorageSpace",
