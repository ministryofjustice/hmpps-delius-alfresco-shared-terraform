{
    "widgets": [
        {
            "type": "metric",
            "x": 6,
            "y": 6,
            "width": 6,
            "height": 6,
            "properties": {
                "view": "timeSeries",
                "stacked": true,
                "metrics": [
                    [
                        "AWS/EC2",
                        "CPUUtilization",
                        "AutoScalingGroupName",
                        "${asg_autoscale_name}"
                    ],
                    [
                        "AWS/AutoScaling",
                        "GroupTotalInstances",
                        ".",
                        "."
                    ]
                ],
                "region": "${region}",
                "title": "Alfresco"
            }
        },
        {
            "type": "metric",
            "x": 0,
            "y": 6,
            "width": 6,
            "height": 6,
            "properties": {
                "metrics": [
                    [
                        "AWS/S3",
                        "BucketSizeBytes",
                        "StorageType",
                        "StandardStorage",
                        "BucketName",
                        "${common_prefix}-storage-s3bucket",
                        {
                            "period": 86400
                        }
                    ],
                    [
                        ".",
                        "NumberOfObjects",
                        ".",
                        "AllStorageTypes",
                        ".",
                        ".",
                        {
                            "period": 86400
                        }
                    ]
                ],
